{
  "name": "alexa-skill-starter-pack-typescript",
  "version": "1.0.0",
  "description": "A sample to create AWS Lambda functions in TypeScript with Serverless.",
  "main": "index.ts",
  "scripts": {
    "start": "nodemon",
    "build": "gulp",
    "tunnel": "ssh -R 80:localhost:3000 serveo.net",
    "clean": "rimraf dist",
    "build:models": "ENV=dev gulp models",
    "get:model:en:dev": "ask api get-model -s amzn1.ask.skill.84dadac8-fa0a-4983-b829-0b2ea9d754ee -l en-US > models/en-US.json --profile=boobo94_dev",
    "get:model:ca:dev": "ask api get-model -s amzn1.ask.skill.84dadac8-fa0a-4983-b829-0b2ea9d754ee -l en-CA > models/en-CA.json --profile=boobo94_dev",
    "get:model:in:dev": "ask api get-model -s amzn1.ask.skill.84dadac8-fa0a-4983-b829-0b2ea9d754ee -l en-IN > models/en-IN.json --profile=boobo94_dev",
    "get:model:au:dev": "ask api get-model -s amzn1.ask.skill.84dadac8-fa0a-4983-b829-0b2ea9d754ee -l en-AU > models/en-AU.json --profile=boobo94_dev",
    "get:model:gb:dev": "ask api get-model -s amzn1.ask.skill.84dadac8-fa0a-4983-b829-0b2ea9d754ee -l en-GB > models/en-GB.json --profile=boobo94_dev",
    "get:models": "npm run get:model:en:dev && npm run get:model:ca:dev && npm run get:model:in:dev && npm run get:model:au:dev && npm run get:model:gb:dev",
    "get:skill": "ask api get-skill --skill-id amzn1.ask.skill.84dadac8-fa0a-4983-b829-0b2ea9d754ee > skill.json --profile=boobo94_dev",
    "deploy:models": "npm run build:models && ask deploy -t model --profile=boobo94_dev --force",
    "deploy:skill": "ask deploy -t skill --profile=boobo94_dev --force",
    "deploy:lambda": "npm run build && ask deploy --target lambda --profile=boobo94_dev --force"
  },
  "keywords": [
    "aws",
    "lambda",
    "tslint",
    "typescript"
  ],
  "author": "Bogdan Alexandru Militaru",
  "repository": {
    "type": "git",
    "url": "https://github.com/boobo94/alexa-skill-starter-pack-typescript.git"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/i18next": "^8.4.3",
    "@types/i18next-sprintf-postprocessor": "^0.0.29",
    "@types/node": "^10.17.2",
    "del": "^4.0.0",
    "express": "^4.16.3",
    "gulp": "^4.0.0",
    "nodemon": "^1.19.4",
    "rimraf": "^2.7.1",
    "ts-node": "^5.0.1",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "ask-sdk": "^2.7.0",
    "ask-sdk-core": "^2.7.0",
    "ask-sdk-model": "^1.22.0",
    "i18next": "^15.1.0",
    "i18next-sprintf-postprocessor": "^0.2.2"
  }
}
